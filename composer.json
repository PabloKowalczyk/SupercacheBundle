{
    "name": "pablok/supercache-bundle",
    "type": "symfony-bundle",
    "description": "Supercache bundle for Symfony",
    "keywords": [
        "cache",
        "pages caching",
        "static cache",
        "supercache"
    ],
    "homepage": "https://github.com/kiler129/SupercacheBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Grzegorz Zdanowski",
            "email": "grzegorz@noflash.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/console": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/filesystem": "~2.8|~3.0|~4.0",
        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/http-foundation": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "voku/html-min": "^3.0.1",
        "voku/simple_html_dom": "^4.0.1"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.2",
        "pds/skeleton": "~1.0",
        "phpunit/phpunit": "^5.7.26",
        "symfony/browser-kit": "~2.8|~3.0|~4.0",
        "symfony/css-selector": "~2.8|~3.0|~4.0",
        "symfony/debug": "~2.8|~3.0|~4.0",
        "symfony/dom-crawler": "~2.8|~3.0|~4.0",
        "symfony/finder": "~2.8|~3.0|~4.0",
        "symfony/phpunit-bridge": "~2.8|~3.0|~4.0",
        "symfony/yaml": "~2.8|~3.0|~4.0"
    },
    "scripts": {
        "post-install-cmd": ["@composer bin all install --ansi -o"],
        "post-update-cmd": ["@composer bin all update --ansi -o"],
        "supercache:checks": [
            "vendor/bin/php-cs-fixer fix -v --diff --dry-run --ansi",
            "vendor/bin/phpstan analyse -l 7 --no-progress --ansi -c phpstan.neon src tests"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PabloK\\SupercacheBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PabloK\\SupercacheBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
